{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "TAMAGUI_TARGET": "native"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "developmentClient": false,
      "env": {
        "TAMAGUI_TARGET": "native",
        "GOOGLE_SERVICES_INFO_PLIST": "./assets/GoogleService-Info.plist"
      },
      "channel": "preview"
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "TAMAGUI_TARGET": "native",
        "GOOGLE_SERVICES_INFO_PLIST": "./assets/GoogleService-Info.plist"
      },
      "distribution": "store",
      "channel": "production"
    }
  }
}
