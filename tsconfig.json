{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src/",
    "paths": {
      "components": [
        "./components"
      ],
      "contexts": [
        "./contexts"
      ],
      "stores": [
        "./stores"
      ],
      "config": [
        "./config"
      ],
      "utils": [
        "./utils"
      ],
      "hooks": [
        "./hooks"
      ],
      "types": [
        "./types"
      ],
      "test": [
        "./test"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
